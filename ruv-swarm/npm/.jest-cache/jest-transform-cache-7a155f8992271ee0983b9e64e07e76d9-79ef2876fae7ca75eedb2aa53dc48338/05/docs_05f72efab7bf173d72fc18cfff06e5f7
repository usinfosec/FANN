        permissions: {
          allow: ['Bash(npx ruv-swarm *)', 'Bash(npm run lint)', 'Bash(npm run test:*)', 'Bash(npm test *)', 'Bash(git status)', 'Bash(git diff *)', 'Bash(git log *)', 'Bash(git add *)', 'Bash(git commit *)', 'Bash(git push)', 'Bash(git config *)', 'Bash(node *)', 'Bash(which *)', 'Bash(pwd)', 'Bash(ls *)'],
          deny: ['Bash(rm -rf /)', 'Bash(curl * | bash)', 'Bash(wget * | sh)', 'Bash(eval *)']
        },
        hooks: {},
        mcpServers: {
          'ruv-swarm': {
            command: 'npx',

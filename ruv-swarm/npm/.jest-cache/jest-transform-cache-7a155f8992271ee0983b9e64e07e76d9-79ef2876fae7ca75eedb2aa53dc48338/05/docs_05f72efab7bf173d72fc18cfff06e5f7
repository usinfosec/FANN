        success: true
      };
    })();
  }

  /**
     * Generate all documentation files
     */
  generateAll() {
    /* istanbul ignore next */
    var _this4 = this;
    return _asyncToGenerator(function* () {
      cov_13hvlacw57().f[4]++;
      cov_13hvlacw57().s[26]++;
      console.log('üìö Generating Claude Code documentation...');
      cov_13hvlacw57().s[27]++;
      try {
        const results =
        /* istanbul ignore next */
        (cov_13hvlacw57().s[28]++, {
          claudeMd:
          /* istanbul ignore next */
          yield _this4.generateClaudeMd(),
